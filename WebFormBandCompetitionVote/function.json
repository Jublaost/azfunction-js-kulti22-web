{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ]
    },
    {
      "name": "votingEntity",
      "type": "table",
      "tableName": "voting",
      "partitionKey": "VOTING",
      "rowKey": "{httpTrigger.body.email}",
      "connection": "StorageConnectionAppSetting",
      "direction": "in"
    },
    {
      "tableName": "voting",
      "connection": "StorageConnectionAppSetting",
      "name": "tableBinding",
      "type": "table",
      "direction": "out"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ],
  "scriptFile": "../dist/WebFormBandCompetitionVote/index.js"
}